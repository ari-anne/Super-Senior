{
    com.badlogic.gdx.graphics.g2d.BitmapFont: {
        title: {
            file: graduate-title.fnt
        }
        large: {
            file: graduate-large.fnt
        }
    }
    com.badlogic.gdx.graphics.Color: {
        black: {
            r: 0
            g: 0
            b: 0
            a: 1
        }
        white: {
            r: 1
            g: 1
            b: 1
            a: 1
        }
    }
    com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
        default: {
            font: title
            fontColor: white
        }
    }
    com.badlogic.gdx.scenes.scene2d.ui.TextButton$TextButtonStyle: {
        header: {
            font: large
            fontColor: white
            up: rectangle
        }
        title: {
            font: title
            fontColor: white
            up: rectangle
        }
    }
    com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
        default: {
            up: circle
            down: circle-click
            disabled: circle
        }
        play: {
            up: right
            down: right-click
        }
        settings: {
            up: settings
            down: settings-click
        }
        leaderboard: {
            up: leaderboard
            down: leaderboard-click
        }
        arrow-left: {
            up: arrow-left
            down: arrow-left-click
        }
        pause: {
            up: pause
            down: pause-click
        }
    }
}